string get_event_message()
{
string r=e.message;
if(r=="")
{
return "";
}
r=string_aes_decrypt(r, net_pass);
return r;
}

void send_reliable(uint peer_id,string message,int channel)
{
message=string_aes_encrypt(message, net_pass);
host.send_reliable(peer_id,message,channel);
}
void send_unreliable(uint peer_id,string message,int channel)
{
message=string_aes_encrypt(message, net_pass);
host.send_unreliable(peer_id,message,channel);
}
