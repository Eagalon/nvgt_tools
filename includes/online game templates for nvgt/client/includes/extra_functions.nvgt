string[] string_to_array(string str,string parser)
{
return str.split(parser);
}
string array_to_string(string[] a,string parser)
{
string final;
for(uint i=0; i<a.length(); i++)
{
final+=a[i]+parser;
}
return final;
}
string randomstring(uint length=10)
{
string temp="abcdefghijklmnopqrstuvwxyz1234567890";
string ret;
for(uint i=0; i<length; i++)
{
ret=ret+temp[random(0,(temp.length()-1))];
}
return ret;
}
